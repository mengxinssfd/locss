._ {
  &.btn:not(.disabled):not(.active):not([disabled]) {
    cursor: pointer;
  }
  &.fit-contain {
    object-fit: contain;
  }
  &.fit-cover {
    object-fit: cover;
  }
  &.img-box {
    > img {
      display: block;
      width: 100%;
      height: 100%;
      @extend ._.fit-cover;
    }
  }
  &.clearfix:after {
    display: block;
    clear: both;
    content: "";
  }
}